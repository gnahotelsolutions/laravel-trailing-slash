{
    "name": "gnahotelsolutions/laravel-trailing-slash",
    "description": "End your Laravel project routes with trailing slash",
    "keywords": [
        "gnahotelsolutions",
        "laravel-trailing-slash"
    ],
    "homepage": "https://github.com/gnahotelsolutions/laravel-trailing-slash",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "David Llop",
            "email": "dllop@gnahs.com",
            "role": "Developer"
        },
        {
            "name": "David Torralbo",
            "email": "dtorralbo@gnahs.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "GNAHotelSolutions\\LaravelTrailingSlash\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GNAHotelSolutions\\LaravelTrailingSlash\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "GNAHotelSolutions\\LaravelTrailingSlash\\Providers\\LaravelTrailingSlashServiceProvider"
            ]
        }
    }
}
